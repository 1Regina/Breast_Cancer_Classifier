			}

			function toggleClass(instance, action) {
				var clsActive = instance.data('icon_active'), clsInactive = instance.data('icon_inactive');

				action = action || 'toggle';
				if('toggle' == action) {
					action = (instance.hasClass('active'))? 'inactive' : 'active';
				}

				var iconDiv = instance.children('span'), contentDiv = instance.siblings('.content_div');

				switch(action) {
					case 'active':
						instance.addClass('active');
						iconDiv.removeClass(clsInactive).addClass(clsActive);
						contentDiv.addClass('show');

						jDoc.bind('click', closeActive);
						break;

					case 'inactive':
						instance.removeClass('active');
						iconDiv.removeClass(clsActive).addClass(clsInactive);
						contentDiv.removeClass('show');

						jDoc.unbind('click', closeActive);
						break;
				}
			}
		});
